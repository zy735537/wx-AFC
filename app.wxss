/**app.wxss**/
@font-face {
  font-family: "iconfont";  
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff')  
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-activity:before { content: "\e6de"; }

.icon-add:before { content: "\e6df"; }

.icon-addition:before { content: "\e6e0"; }

.icon-close:before { content: "\e6e9"; }

.icon-delete_fill:before { content: "\e6f2"; }

.icon-empty_fill:before { content: "\e6f7"; }

.icon-group:before { content: "\e6ff"; }

.icon-mine:before { content: "\e70e"; }

.icon-people:before { content: "\e715"; }

.icon-scan:before { content: "\e722"; }

.icon-success_fill:before { content: "\e72d"; }

page, main{
  height: 100%;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  color: #F9B82E;
  background-color: #F2F2F2;
}

.page-title {
  display: flex;
  flex-direction: column;
  align-items: center;
  vertical-align: middle;
  padding: 10rpx 30rpx;
}

.container {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}

.row {
  display: block;
  margin: 0px;
}

.col {
  display: flex;
  /* font-family: -apple-system-font, "Helvetica Neue", sans-serif; */
  font-size: 17px;
}

.col>.col-1, .col-2, .col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9,
.col-10, .col-11, .col-12 {
  overflow: hidden;
}

.col-1 {
  width: 8.33333333333333%;
}

.col-offset-1 {
  margin-left: 8.33333333333333%;
}

.col-2 {
  width: 16.6666666666666%;
}

.col-offset-2 {
  margin-left: 16.6666666666666%;
}

.col-3 {
  width: 25%;
}

.col-4 {
  width: 33.3333333333333%;
}

.col-5 {
  width: 41.6666666666666%;
}

.col-6 {
  width: 50%;
}

.col-7 {
  width: 58.333333333333333%;
}

.col-8 {
  width: 66.66666666666666%;
}

.col-9 {
  width: 75%;
}

.col-10 {
  width: 83.33333333333333%;
}

.col-11 {
  width: 91.66666666666666%;
}

.col-12 {
  width: 100%;
}

.x-center {
  text-align: center;
}

button {
  height: 120rpx;
  border-radius: 0;
  border: none;
  padding-top: 10rpx;
}

  button::after {
    border: 0; 
  }

.primary {
  background-color: #F9B82E;
  color: #fff;
}

  .primary.button-hover {
    /* background-color: #F9B82E; */
    background-color: #f6ac10;
  }

.form {
  display: flex;
  flex-direction: column;  
  padding: 100rpx 80rpx;
  color: #333;
}

.form .form-group {
  margin: 10rpx 0 40rpx;
}

.form-label {  
  display: block;
  font-weight: normal;  
  margin-bottom: 10rpx;
}

.form-control {
  background-color: #fff;
  border: 1px solid #ddd;
  color: #333;
  font-weight: normal;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  padding: 20rpx 30rpx;
  /* margin: 10rpx 0 40rpx; */
}

.title{
  padding: 30rpx;
  font-size: 50rpx;
}

.rankList{
  display: flex;
  align-items: center;
}

.rankItem {
  background-color: #fff;
  display: flex;
  padding: 20rpx 30rpx;
  margin-bottom: 10rpx;
}

.rankItem.person-rank,
.rankItem.person-rank-sm {
  background-color: #363636;
}  

.rankItem.me {
  background-color:#F2F2F2;
} 

.rankItem .item {  
  align-self: center;
  text-align: center;
  vertical-align: middle; 
  font-size: 34rpx;  
}

.rankItem .item.userinfo-avatar {
  width: 110rpx;
  height: 110rpx;
}

.rankItem.person-rank .item.userinfo-avatar {
  width: 190rpx;
  height: 190rpx;
}

.rankItem .item.rank {
  font-size: 40rpx;
  font-weight: bolder;
}

.rankItem.person-rank .item.rank,
.rankItem.person-rank .item.overall,
.rankItem.person-rank-sm .item.rank,
.rankItem.person-rank-sm .item.overall {
  font-size: 50rpx;
  font-weight: bolder;
}

.rankItem .item.rank::after,
.rankItem .item.overall::after {
  content: attr(data-suffix);
  font-weight: normal;
  font-size: 30rpx;
}

.rankItem .item.overall {  
  font-size: 36rpx;
}

.rankItem .item.name-detail {
  padding-left: 10rpx;
}

.rankItem .item.name-detail .name{
  align-self: center;
  text-align: left;
  display: block;
}

.rankItem .item.name-detail .detail{
  align-self: center;
  text-align: left;
  display: block;
  font-size: 24rpx;
  color: #C1C1C1;
}

.gameinfo .name-detail {
  display: flex;
  align-self: center;
  flex-direction: column;  
}

.gameinfo .name-detail .title {
  display: block;
  align-self: flex-start;  
  padding: 0 20rpx;  
}

.gameinfo .name-detail .detail {
  align-self: flex-start;  
  padding: 0 20rpx;
  display: block;
  font-size: 24rpx;
  color: #C1C1C1;  
}

.gameinfo .header-right {
  align-self: flex-end;
  align-items: flex-end;
  flex-direction: row;  
  display: flex;  
  width: 120rpx;
  height: 110rpx;
}

.gameinfo .header-right .iconfont {
  font-size: 60rpx;
  width: 100%;
  align-self: center;
  text-align: right;
}

.userinfo-avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 0 10rpx;
  border-radius: 50%;
  border: 1px solid #ccc;
}

.gameinfo-avatar {
  width: 110rpx;
  height: 110rpx;
  /* margin: 20rpx; */
  border-radius: 50%;
}

.deactive {
  color: #c1c1c1;  
  opacity: 0.5;
}

.no-data {
  padding: 30rpx;
  text-align: center;  
}